<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../Styles/login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.benedict.minibank.Controllers.LoginController">
   <children>
      <VBox prefHeight="400.0" prefWidth="200.0" styleClass="login_logo_container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mini bankas" />
         </children>
      </VBox>
      <VBox layoutX="224.0" layoutY="50.0" prefHeight="352.0" prefWidth="350.0" styleClass="login_form_container" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label text="Pasirinkite vartotojo tipą" />
            <ChoiceBox fx:id="acc_selector" prefWidth="100.0" styleClass="account_selector" />
            <Label fx:id="payee_address_lbl" text="Mokėtojo adresas" />
            <TextField fx:id="payee_address_fld" />
            <Label fx:id="password_lbl" text="Slaptažodis" />
            <PasswordField fx:id="password_fld" />
            <Button fx:id="login_btn" mnemonicParsing="false" text="Prisijungti" />
            <Label fx:id="error_lbl" styleClass="error_lbl" text="" />
         </children>
      </VBox>
   </children>
</AnchorPane>
